generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Animal {
  id        Int     @id @default(autoincrement())
  name      String
  speciesId Int

  @@map("animals")
}

model Species {
  id   Int    @id @default(autoincrement())
  name String

  @@map("species")
}

model Camera {
  id       Int    @id @default(autoincrement())
  location String

  @@map("cameras")
}

model Alert {
  id      Int    @id @default(autoincrement())
  message String

  @@map("alerts")
}
